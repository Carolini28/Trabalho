<GridPane alignment="CENTER" hgap="10.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="trabalho.FXMLDocumentControllerCliente">
  <Label text="ID:" GridPane.columnIndex="1" GridPane.rowIndex="1" />
